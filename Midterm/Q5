clc
clear all

x = linspace(0, 4*pi, 500);

y1=sin(x);
y2=exp(-0.1*x);

figure;

yyaxis left;

plot(x,y1,'-b','LineWidth',1.5);
hold on;
ylabel('Y value left');

yyaxis right;
plot(x,y2,'g');
ylabel('title right')

hold off

legend('sin(x)','exp(-0.1*x)');
grid on;
title('Elad title');
xlabel('X count');

